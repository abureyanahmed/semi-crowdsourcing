{
    "name": "Citation chrome extension",
    "description" : "Extracts citation information",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": "citation_clipart.png"
    },
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "version": "1.0",
    "manifest_version": 2
}
